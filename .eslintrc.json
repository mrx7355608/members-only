{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "quotes": ["error", "double"],
      "space-before-function-paren": "off",
      "require-jsdoc": "off",
      "object-curly-spacing": "off",
      "indent": "off"
    }
}
